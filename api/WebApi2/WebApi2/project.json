{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "webapi2",
  "projectType": "application",
  "targets": {
    "build": {
      "options": {
        "configuration": "Release",
        "noDependencies": false
      }
    },
    "lint": {
      "cache": true,
      "options": {
        "fix": true,
        "fixWhitespace": true,
        "fixStyle": "warn",
        "fixAnalyzers": "warn",
        "report": "./.reports",
        "verbosity": "diagnostic"
      }
    }
  }
}
